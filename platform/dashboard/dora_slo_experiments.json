{
  "title": "StratMaster Delivery & Reliability Overview",
  "widgets": [
    {
      "type": "timeseries",
      "metric": "deploy_frequency",
      "source": "ci-pipeline",
      "target": ">= 5/week"
    },
    {
      "type": "timeseries",
      "metric": "lead_time",
      "source": "orchestration/planning",
      "target": "<= 2 days"
    },
    {
      "type": "timeseries",
      "metric": "error_budget_burn",
      "source": "RELIABILITY/SLOs.yml",
      "target": "< 1.0"
    },
    {
      "type": "table",
      "metric": "experiment_velocity",
      "source": "EXPERIMENTS/playbooks",
      "columns": ["experiment", "status", "uplift", "decision"]
    },
    {
      "type": "table",
      "metric": "adr_cadence",
      "source": "docs/architecture/adr",
      "columns": ["id", "title", "status", "last_updated"]
    }
  ]
}
