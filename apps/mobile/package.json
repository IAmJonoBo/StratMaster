    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "babel-jest": "^30.0.0",
    "eslint": "^9.0.0",
    "jest": "^30.0.0",
    "metro-react-native-babel-preset": "0.77.0",
