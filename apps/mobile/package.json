{
  "name": "stratmaster-mobile",
  "version": "1.0.0",
  "description": "StratMaster Mobile App for Stakeholder Approvals",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
